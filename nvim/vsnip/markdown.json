{
  "Markdown front matter": {
    "prefix": "meta",
    "body": [
      "---",
      "title: $1",
      "layout: default",
      "comments: true",
      "published: true",
      "---",
      "",
      "# $1",
      "",
      "$0"
    ]
  },
  "link": {
    "prefix": "link",
    "body": [
      "[$1](https://$2)"
    ]
  },
  "image": {
    "prefix": "image",
    "body": [
      "![$1]($2)"
    ]
  },
  "table": {
    "prefix": "table",
    "body": [
      "|$1|$2|",
      "|---|---|",
      "|$3|$4|"
    ]
  }
}
